# Example environment configuration for pws-con
# Copy this file to .env and replace with your actual values

VIPPS_CLIENT_ID=your_vipps_client_id_here
VIPPS_CLIENT_SECRET=your_vipps_client_secret_here
VIPPS_SUBSCRIPTION_KEY=your_vipps_subscription_key_here
VIPPS_MERCHANT_SERIAL_NUMBER=your_merchant_serial_number_here

VIPPS_SYSTEM_NAME=acme
VIPPS_SYSTEM_VERSION=3.1.2
VIPPS_PLUGIN_NAME=acme-webshop
VIPPS_PLUGIN_VERSION=4.5.6

# React App Configuration
REACT_APP_BACKEND_URL=http://localhost:4000

# Firebase Configuration (Add your Firebase config here)
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id
